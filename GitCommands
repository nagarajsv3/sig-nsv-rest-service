--create a new repository on the command line
echo "# springframework" >> README.md
git init
git add README.md
git commit -m "first commit"
git remote add origin https://github.com/nagarajsv3/springframework.git
git push -u origin master

--push an existing repository from the command line
git remote add origin https://github.com/nagarajsv3/springframework.git
git push -u origin master


-- change the remote repository
it remote set-url origin https://github.com/nagarajsv3/springframeworkapplearning8818.git
